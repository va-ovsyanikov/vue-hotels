{
  "name": "hotels-rules",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "server": "json-server --watch rules.json --port=3001",
    "start": "concurrently \"npm run server\"  \"npm run dev\" "
  },
  "dependencies": {
    "axios": "^1.5.0",
    "concurrently": "^8.2.1",
    "json-server": "^0.17.3",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "prettier": "^3.0.0",
    "vite": "^4.4.9"
  }
}
